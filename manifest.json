{
    "name": "Boob_Replacer",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Replaces the synonyms of boobs with link to a pornhub.com",
    "content_scripts": [{
      "matches": ["\u003Call_urls>"],
      "js": ["src/jquery.min.js"]
      }],  
      "background": {
        "scripts": ["src/Background.js"]
      },
      "browser_action": {
        "default_icon": "images/logo.jpg",
        "default_title": "Click to Enable!"
      },
      "permissions": [
        "*://*/*",
        "storage",
        "notifications"
      ] 
}